algoritmo "NomeESalarioFuncionario"
var
   nome, data: caractere
   sal: real
inicio
      Escreva("Nome do funcionário: ")
      leia(nome)
      Escreva("Salário: ")
      leia(sal)
      Escreva("Data: ")
      leia(data)
      Escreva("O funcionário ", nome, " tem um salário de", sal, " em ", data)
fimalgoritmo

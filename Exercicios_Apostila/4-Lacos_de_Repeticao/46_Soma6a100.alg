algoritmo "Soma6a100"
var
   c, soma: inteiro
inicio

      c <- 6
      
      soma <- c
      
      Enquanto c <= 10 faca

               Escreva(c)
               
               c <- c + 2
      
               soma <- soma + c
      
      FimEnquanto
      
      EscrevaL("A soma da contagem e: ", soma)
      
fimalgoritmo

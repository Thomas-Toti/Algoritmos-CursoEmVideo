algoritmo "NumeroInteiroEcontagem"
var
    n, c: inteiro
inicio

      Escreva("Informe um numero inteiro e positivo: ")
      leia(n)
      
      c <- 0
      
      Enquanto c < n faca

               c <- c + 1

               EscrevaL(c)
               
      FimEnquanto
      
      Escreva(" Acabou!")
fimalgoritmo

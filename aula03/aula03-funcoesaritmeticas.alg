algoritmo "funcoesAritmeticas"
var
a: inteiro
b, c, d, e, f: real
inicio
a <- abs(-50)
escreval("a: ", a)

b <- exp(9, 5)
escreval("b: ", b)

c <- raizQ(81)
escreval("c: ", c)

d <- int(raizQ(90))
escreval("c: ", d)

e <- GraupRad(90)
 escreval("e: ", e)

f <- sen(GradpRad(90))
escreval("f: ", f)
fimalgoritmo
